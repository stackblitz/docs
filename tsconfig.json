{
  "compilerOptions": {
    "target": "ES2018",
    "moduleResolution": "node",
    "jsx": "preserve",
    "paths": {
      "$/*": ["./docs/.vitepress/*"]
    }
  },
  "vueCompilerOptions": {
    "experimentalAllowTypeNarrowingInInlineHandlers": true
    //"experimentalDisableTemplateSupport": true
  },
  "include": ["docs/.vitepress/**/*", "docs/**/*.md"],
  "exclude": ["node_modules/"]
}
